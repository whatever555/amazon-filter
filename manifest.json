{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Amazon Global Shipping Filter",
  "description": "Filter Amazon search (if item can be shipped to your location)",
  "version": "263.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
          "19": "icon19.png",
           "38": "icon38.png",
          "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
     "matches": [
         "http://*.amazon.co.jp/*",
         "https://*.amazon.co.jp/*",
         "http://*.amazon.in/*",
         "https://*.amazon.in/*",
         "http://*.amazon.ca/*",
         "https://*.amazon.ca/*",
         "http://*.amazon.it/*",
         "https://*.amazon.it/*",
         "http://*.amazon.es/*",
         "https://*.amazon.es/*",
         "http://*.amazon.de/*" ,
         "https://*.amazon.de/*" ,
         "http://*.amazon.co.uk/*",
         "https://*.amazon.co.uk/*",
         "http://*.amazon.fr/*",
         "https://*.amazon.fr/*",
         "http://*.amazon.cn/*",
         "https://*.amazon.cn/*",
         "http://*.amazon.nl/*",
         "https://*.amazon.nl/*",
         "http://*.amazon.com.au/*",
         "https://*.amazon.com.au/*",
         "http://*.amazon.com.sg/*",
         "https://*.amazon.com.sg/*",
         "http://*.amazon.com.mx/*",
         "https://*.amazon.com.mx/*",
         "http://*.amazon.com.br/*",
         "https://*.amazon.com.br/*",
         "http://*.amazon.com/*",
         "https://*.amazon.com/*"  
     ],
      "css": ["mystyle.css"],
      "js": ["jq.js", "attrchange.js", "myscript.js"]
    }
  ],
  "browser_action": {
    "default_title": "Toggle filter on/off",
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
